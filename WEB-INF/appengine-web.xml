<?xml version="1.0" encoding="utf-8"?>
<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <threadsafe>true</threadsafe>
  <application>endless-matter-364521</application>
  <async-session-persistence enabled="true" />
  <sessions-enabled>true</sessions-enabled>
  <runtime>java8</runtime>
	<system-properties>
	<property name="java.util.logging.config.file" value="WEB-INF/logging.properties" />
	<property name="file.encoding" value="UTF-8" />
	<property name="DEFAULT_ENCODING" value="UTF-8" />
	</system-properties>
	 <static-files>
	     <include path="/**.png" expiration="30d" />
	     <include path="/**.js" expiration="30d" />
	     <include path="/**.css" expiration="30d" />
	     <include path="/**.jpg" expiration="30d" />
	     <include path="/**.png" expiration="30d" />
	     <include path="/**.*" />
    </static-files> 
     <automatic-scaling>
                <max-concurrent-requests>30</max-concurrent-requests>
                <min-instances>0</min-instances>
                <max-instances>10</max-instances>
                <min-idle-instances>1</min-idle-instances>
                <min-pending-latency>1s</min-pending-latency>
                <max-pending-latency>3s</max-pending-latency>
            </automatic-scaling>
</appengine-web-app>

